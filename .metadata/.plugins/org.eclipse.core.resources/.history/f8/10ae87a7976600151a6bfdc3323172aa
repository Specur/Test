package gramy;

import javax.swing.JComponent;
import javax.swing.JPanel;

public class Badania extends JPanel{
	

	public Badania(){
		setLayout(null);
		
		
		JComponent Opisy = new BadaniaOpisy();
		add(Opisy);
		
		JComponent Przyciski = new BadaniaPrzyciski();
		add(Przyciski);
		
		JPanel Obrazki = new ObrazkiBadania();
		add(Obrazki);
		
		
		
		setBounds(0, 0, Stage.SZEROKOSC , Stage.WYSOKOSC);
		setVisible(true);
		
	}

}
