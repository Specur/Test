package gramy;

import javax.swing.JComponent;
import javax.swing.JPanel;
import javax.swing.JScrollPane;

public class Budynki extends JScrollPane{
	
	public Budynki(){
		
		setLayout(null);
		
		
		
		JComponent BudynkiOpisy = new BudynkiOpisy();
		add(BudynkiOpisy);
		
		JPanel Budynki = new BudynkiObrazki();
		add(Budynki);
		
		JComponent Przyciski = new BudynkiPrzyciski();
		add(Przyciski);
		
		setBounds(1,1,Stage.SZEROKOSC,Stage.WYSOKOSC);
		setVisible(true);
		
	}

}
