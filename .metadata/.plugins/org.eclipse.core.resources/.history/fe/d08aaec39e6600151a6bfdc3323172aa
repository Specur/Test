package gramy;

import javax.swing.JComponent;
import javax.swing.JPanel;
import javax.swing.JScrollPane;

public class Budynki extends JPanel{
	
	public Budynki(){
		
		setLayout(null);
		
		
		
		JComponent BuilidDescription = new BudynkiOpisy();
		add(BuilidDescription);
		
		JComponent Button = new BudynkiPrzyciski();
		add(Button);
		
		JPanel Budynki = new BudynkiObrazki();
		add(Budynki);
		
		
		
		setBounds(1,1,Stage.SZEROKOSC,Stage.HEIGHT);
		setVisible(true);
		
	}

}
