package gramy;

import javax.swing.JPanel;
import javax.swing.JScrollPane;

public class Budynki extends JScrollPane{
	
	public Budynki(){
		JScrollPane sp=new JScrollPane();
		sp.setLayout(null);
		
		JPanel Budynki = new BudynkiObrazki();
		sp.add(Budynki);
		
		
		sp.setBounds(1,1,Stage.SZEROKOSC,Stage.WYSOKOSC);
		sp.setVisible(true);
		
	}

}
