package gramy;

import javax.swing.JComponent;
import javax.swing.JPanel;

public class Przeglad extends JPanel{
	public Przeglad(){
		setLayout(null);
		
		JComponent roz = new PrzyciskiPrzeglad();
		add(roz);
		JComponent dwo = new OpisyPrzeglad();
		add(dwo);
		JPanel tri = new ObrazkiPrzeglad();
		add(tri);
		setBounds(0, 0, Stage.SZEROKOSC , Stage.WYSOKOSC);
		setVisible(true);
		
		
	}

}
