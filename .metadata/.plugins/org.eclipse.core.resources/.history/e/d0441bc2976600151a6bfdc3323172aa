package gramy;

import javax.swing.JComponent;
import javax.swing.JFrame;
import javax.swing.JPanel;

public class Flota extends JPanel{
	
	public Flota(){
		setLayout(null);
		
		JComponent Opisy = new FlotaOpisy();
		add(Opisy);
		
		JPanel Obrazki = new FlotaObrazki();
		add(Obrazki);
		
		
		
		setBounds(0, 0, Stage.SZEROKOSC , Stage.WIDTH);
		setVisible(true);
	}

}
